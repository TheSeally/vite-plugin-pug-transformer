{
  "name": "vite-plugin-pug-transformer",
  "version": "1.0.0",
  "description": "Vite plugin for supporting pug templates",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=12.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheSeally/vite-plugin-pug-transformer.git"
  },
  "keywords": [
    "vite-plugin",
    "pug",
    "vite"
  ],
  "author": "TheSeally",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheSeally/vite-plugin-pug-transformer/issues"
  },
  "homepage": "https://github.com/TheSeally/vite-plugin-pug-transformer#readme",
  "devDependencies": {
    "eslint": "^8.8.0"
  },
  "dependencies": {
    "pug": "^3.0.2"
  },
  "peerDependencies": {
    "vite": "^2.5.10"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "prefer-const": "error"
    }
  }
}
